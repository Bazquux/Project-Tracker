<!DOCTYPE html>
<html>
<head>
  <title><%= @title || "Project Tracker" %></title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% flash.each do |key , value| %>
	<div id="<%= key %>"> <%= value%> </div>
<% end %>
<h1><%= link_to "Ticketee", root_path %></h1>

<div id="nav">
	<%= render 'layouts/nav'%>
</div>
<%= yield %>

</body>
</html>
